.tErrorView{
  align-items: center;
  padding: 10%;
  justify-content: center;
}
.tError_container{
  display: flex;
  justify-content: center;
  font-size: 20px;
  color: #666;
}
.tError_context{
  margin-top: 40px;
  display: flex;
  justify-content: center;
}
.tError_detail{
  border: 1px solid #eee;
  background-color: #eee;
  border-radius: 4px;
  justify-content: center;
  height: 100px;
  margin-top: 20px;
}
.textType{
  color: #666;
  font-size: 20px;
  display: block;
  text-align: center;
}
.detailType{
  font-size: 14px;
  color: #333;
  display: block;
  text-align: center;
  margin: 10px;
}
.retryBtn{
  background-color: #62b900;
  color: #fff;
  margin-top: 40px;
  width: 100%;
}