.hasSignView{
  align-items: center;
  padding: 10%;
  justify-content: center;
}
.hasSign_container{
  display: flex;
  justify-content: center;
  font-size: 20px;
  color: #666;
}
.hasSign_context{
  margin-top: 40px;
  display: flex;
  justify-content: center;
}
.time_context,.order_context{
  margin-top: 10px;
  display: flex;
  justify-content: center;
}
.textType{
  color: #666;
  font-size: 20px;
  display: block;
  text-align: center;
}
.timeType,.orderType{
  color: #666;
  font-size: 16px;
  display: block;
  text-align: center;
}
.signBtn{
  background-color: #62b900;
  color: #fff;
  margin-top: 20px;
  width: 60%;
}
.cancelBtn{
  background-color: #ff0000;
  color: #fff;
  margin-top: 20px;
  width: 60%;
}
.backBtn{
  background-color: #aaaaaa;
  color: #fff;
  margin-top: 20px;
  width: 60%;
}